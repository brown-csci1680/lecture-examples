    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch sender",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/tcp-ip-send",
            "console": "integratedTerminal",
            "args": ["5000", "127.0.0.1", "5001", "aaaaaaaaaaaa"]
        },
        {
            "name": "Launch receiver",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/tcp-ip-recv",
            "console": "integratedTerminal",
            "args": ["5001"]
        },
    ]
}